    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python: Debug sweep.py",
            "type": "debugpy",
            "request": "launch",
            "program": "/work/mstreicher/ida_nitrogen_prediction/src/sweep.py",
            "console": "integratedTerminal",
            "args": [
                "--model", "PLSR",
                "--n_components", "10",
                "--use_wandb", "True",
                ]
        }
    ]
}